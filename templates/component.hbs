import cn from "classnames";
import type { FC, ReactNode } from "react";

import styles from "./{{> ComponentName}}.pcss";

type Props = {
    prop: number;
    onClick?: () => void;
    className?: string;
    children: ReactNode;
};

export const {{> ComponentName}}: FC<Props> = ({
    prop,
    onClick,
    className,
    children,
}) => {
    return (
        <div
            onClick={onClick}
            className={cn(styles.{{> CssClassName}}, className)}
        >
            {prop}
            {children}
        </div>
    );
};
